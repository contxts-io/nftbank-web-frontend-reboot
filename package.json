    "@thirdweb-dev/sdk": "^4.0.15",
    "@types/jsonwebtoken": "^8.5",
    "@types/node": "20.5.7",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@types/react-modal": "^3.16.0",
    "apexcharts": "^3.42.0",
    "autoprefixer": "10.4.15",
    "axios": "^1.5.0",
    "chart.js": "^4.4.0",
    "cookies-next": "^3.0.0",
    "cypress": "^13.0.0",
    "eslint": "8.48.0",
    "eslint-config-next": "13.4.19",
    "ethers": "^5",
    "firebase": "^10.3.1",
    "jotai": "^2.4.0",
    "jsonwebtoken": "^8.5",
    "lokijs": "1.x",
    "next": "13.4.19",
    "next-themes": "^0.2.1",
    "pino-pretty": "^10.2.3",
